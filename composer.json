{
    "name": "guillaumejust/tracktik",
    "description": "PHP developer evaluation",
    "authors": [
        {
            "name": "Guillaume JUST",
            "email": "just.guillaume@hotmail.fr"
        }
    ],
    "require": {
        "php": "7.2",
        "ext-http": "*"
    },
    "autoload": {
        "psr-4": { "App\\": "src/" }
    },
    "scripts": {
        "test": "./vendor/bin/phpunit --bootstrap vendor/autoload.php tests",
        "coverage": "./vendor/bin/phpunit --coverage-html coverage/ --whitelist src/ --bootstrap vendor/autoload.php tests"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5"
    }
}
